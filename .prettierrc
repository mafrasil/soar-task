{
  "plugins": [
    "prettier-plugin-classnames",
    "prettier-plugin-tailwindcss"
  ],
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "printWidth": 100,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "trailingComma": "es5",
  "endOfLine": "lf"
}
